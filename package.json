{
  "name": "htmlhint-loader",
  "version": "1.1.0",
  "description": "A webpack loader for htmlhint",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && mocha",
    "coverage": "istanbul cover -x **test** _mocha -- -R spec && cat ./coverage/lcov.info | coveralls",
    "lint": "eslint index.js test/test.spec.js",
    "start": "mocha --watch",
    "preversion": "npm test",
    "postversion": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattlewis92/htmlhint-loader.git"
  },
  "keywords": [
    "webpack",
    "loader",
    "htmlhint",
    "linter",
    "lint",
    "html"
  ],
  "author": "Matt Lewis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattlewis92/htmlhint-loader/issues"
  },
  "homepage": "https://github.com/mattlewis92/htmlhint-loader#readme",
  "dependencies": {
    "chalk": "^1.1.0",
    "htmlhint": "^0.9.7",
    "loader-utils": "^0.2.10"
  },
  "devDependencies": {
    "chai": "~3.5.0",
    "coveralls": "~2.11.2",
    "eslint": "~3.4.0",
    "eslint-config-mwl": "~0.5.0",
    "istanbul": "~0.4.5",
    "mocha": "~3.0.2",
    "raw-loader": "~0.5.1",
    "rewire": "~2.5.2",
    "sinon": "~1.17.5",
    "sinon-chai": "~2.8.0",
    "strip-ansi": "~3.0.1",
    "webpack": "~1.13.2"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "files": [
    "index.js"
  ]
}
